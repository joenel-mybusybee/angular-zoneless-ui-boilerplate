{
  "name": "angular-zoneless-ui",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:staging": "ng build --configuration staging",
    "build:local": "ng build --configuration development",
    "deploy:gh": "ng build --configuration=staging && ngh --dir=dist/staging/browser",
    "watch": "ng build --watch --configuration development",
    "prettify": "prettier --config ./.prettierrc --write \"src/**/*.{ts,html,scss}\"",
    "test": "ng test"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@swimlane/ngx-datatable": "^22.0.0-alpha.1",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "jwt-decode": "^4.0.0",
    "pnpm": "^10.14.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.2"
  }
}
